# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Supabase Connection Settings
SUPABASE_MAX_RETRIES=3
SUPABASE_RETRY_DELAY=1000
SUPABASE_CONNECTION_TIMEOUT=10000
SUPABASE_ENABLE_REALTIME=true
SUPABASE_ENABLE_AUTH=true

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres
DATABASE_MAX_CONNECTIONS=10
DATABASE_CONNECTION_TIMEOUT=5000

# Authentication Configuration
NEXTAUTH_SECRET=your_nextauth_secret
NEXTAUTH_URL=http://localhost:3000

# OAuth Providers (Optional)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Storage Configuration
NEXT_PUBLIC_STORAGE_BUCKET=video-clipper-storage

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Video Processing Configuration
MAX_FILE_SIZE=50MB
PROCESSING_TIMEOUT=300000
PROCESSING_MAX_CONCURRENT=5
TEMP_STORAGE_PATH=/tmp/video-clipper

# Feature Flags
NEXT_PUBLIC_ENABLE_TEAMS=true
NEXT_PUBLIC_ENABLE_TEMPLATES=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true

# Development Configuration
NODE_ENV=development
NEXT_PUBLIC_DEBUG=false
# F
ile Cleanup Configuration
ADMIN_CLEANUP_KEY=your_secure_cleanup_key_here
CLEANUP_URL=http://localhost:3002/api/v1/admin/cleanup